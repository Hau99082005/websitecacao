# Turn on URL rewriting
RewriteEngine On

# Set the base URL to /phung/ if the project is in a subfolder
RewriteBase /phung/admin/index.php
# Ensure the request is not for an existing file or directory
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Redirect all requests to phung/admin/index.php
RewriteRule ^(.*)$ phung/admin/index.php [L]
